{
    "name": "taoceanz/silverstripe-instance-shortcodes",
    "type": "silverstripe-vendormodule",
    "description": "Facilitate using shortcodes, but rendered in instance context (rather than static) giving it access to the model data of the page.",
    "homepage": "https://github.com/taoceanz/silverstripe-instance-shortcodes",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "shortcode",
        "extension",
        "module",
        "page",
        "dataobject"
    ],
    "authors": [
        {
            "name": "Thomas Ocean",
            "email": "do@taocean.io",
            "role": "developer"
        }
    ],
    "support":  {
        "issues": "https://github.com/taoceanz/silverstripe-instance-shortcodes/issues"
    },
    "require": {
      "silverstripe/cms": "^4",
      "silverstripe/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "TAOCEANZ\\Core\\": "src/Core",
            "TAOCEANZ\\ORM\\": "src/ORM"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
